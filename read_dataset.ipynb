</span><span class="s1">import </span><span class="s0">tensorflow </span><span class="s1">as </span><span class="s0">tf</span>
<span class="s1">import </span><span class="s0">zipfile</span><span class="s5">, </span><span class="s0">tarfile</span>
<span class="s1">from </span><span class="s0">pathlib </span><span class="s1">import </span><span class="s0">Path</span>
<span class="s1">import </span><span class="s0">pandas </span><span class="s1">as </span><span class="s0">pd</span>

<span class="s1">def </span><span class="s0">load_file(url</span><span class="s5">, </span><span class="s0">dest=</span><span class="s3">&quot;data&quot;</span><span class="s0">):</span>
    <span class="s0">dest = Path(dest)</span>
    <span class="s0">dest.mkdir(parents=</span><span class="s1">True</span><span class="s5">, </span><span class="s0">exist_ok=</span><span class="s1">True</span><span class="s0">)</span>

    <span class="s0">local_path = tf.keras.utils.get_file(</span>
        <span class="s0">fname=url.split(</span><span class="s3">&quot;/&quot;</span><span class="s0">)[-</span><span class="s4">1</span><span class="s0">]</span><span class="s5">,</span>
        <span class="s0">origin=url</span><span class="s5">,</span>
        <span class="s0">cache_dir=str(dest)</span><span class="s5">,</span>
        <span class="s0">cache_subdir=</span><span class="s3">&quot;.&quot;</span><span class="s5">,</span>
        <span class="s0">extract=</span><span class="s1">False</span>
    <span class="s0">)</span>
    <span class="s0">local_path = Path(local_path)</span>

    <span class="s2"># ZIP</span>
    <span class="s1">if </span><span class="s0">local_path.suffix == </span><span class="s3">&quot;.zip&quot;</span><span class="s0">:</span>
        <span class="s1">with </span><span class="s0">zipfile.ZipFile(local_path</span><span class="s5">, </span><span class="s3">&quot;r&quot;</span><span class="s0">) </span><span class="s1">as </span><span class="s0">z:</span>
            <span class="s0">z.extractall(dest)</span>

    <span class="s2"># TGZ / TAR.GZ</span>
    <span class="s1">elif </span><span class="s0">local_path.suffix </span><span class="s1">in </span><span class="s0">[</span><span class="s3">&quot;.tgz&quot;</span><span class="s5">, </span><span class="s3">&quot;.gz&quot;</span><span class="s0">] </span><span class="s1">or </span><span class="s3">&quot;&quot;</span><span class="s0">.join(local_path.suffixes) == </span><span class="s3">&quot;.tar.gz&quot;</span><span class="s0">:</span>
        <span class="s1">with </span><span class="s0">tarfile.open(local_path</span><span class="s5">, </span><span class="s3">&quot;r:*&quot;</span><span class="s0">) </span><span class="s1">as </span><span class="s0">tar:</span>
            <span class="s0">tar.extractall(dest)</span>

    <span class="s1">return </span><span class="s0">dest</span>

